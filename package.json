{
  "name": "bob",
  "version": "1.0.0",
  "description": "Pixel-perfect mirrored site served by Next.js",
  "scripts": {
    "generate:mirror-runtime-bundle": "node scripts/generate_mirror_runtime_bundle.mjs",
    "prebuild": "pnpm run generate:mirror-runtime-bundle",
    "predev": "pnpm run generate:mirror-runtime-bundle",
    "build": "next build",
    "dev": "next dev -p 4322",
    "start": "next start -p 4322",
    "tina:dev": "pnpm run generate:mirror-runtime-bundle && TINA_PUBLIC_IS_LOCAL=true tinacms dev -c \"next dev -p 4323\"",
    "tina:build": "pnpm run generate:mirror-runtime-bundle && TINA_PUBLIC_IS_LOCAL=true tinacms build --local --skip-cloud-checks",
    "sync:assets": "python scripts/sync_assets.py",
    "sync:media": "python scripts/sync_media_assets.py",
    "clone:live": "bash scripts/clone_live_site.sh",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "next": "^16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.16.5",
    "@tinacms/cli": "^2.1.6",
    "tinacms": "^3.5.0",
    "wrangler": "^4.67.0"
  },
  "packageManager": "pnpm@10.28.0"
}
